            android:label="@string/title_activity_my_top_bar" />
        <activity
            android:name=".asynctask.NetWorkAsynctaskActivity"
            android:label="@string/title_activity_net_work_asynctask" />
        <activity
            android:name=".activeandroid.ActiveAndroidActivity"
            android:label="@string/title_activity_active_android" />

        <!-- active record -->
        <meta-data
            android:name="AA_DB_NAME"
            android:value="myproject.db" />
        <meta-data
            android:name="AA_DB_VERSION"
            android:value="1" />

        <activity
            android:name=".duandianxuchuan.DownloadActivity"
            android:label="@string/title_activity_download" />
        <activity android:name=".zhihutupianxuanzeqi.ZhiHuTuPianActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
    </application>

</manifest>