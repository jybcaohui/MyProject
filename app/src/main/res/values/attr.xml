<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="item_count" format="integer"/>
    <attr name="weiicon" format="reference" />
    <attr name="weicolor" format="color" />
    <attr name="text" format="string" />
    <attr name="text_size" format="dimension" />

    <declare-styleable name="ViewPagerIndicator">
        <attr name="item_count" />
    </declare-styleable>

    <declare-styleable name="ChangeColorIconWithTextView">
        <attr name="weiicon" />
        <attr name="weicolor" />
        <attr name="text" />
        <attr name="text_size" />
    </declare-styleable>
    <declare-styleable name="TagFlowLayout">
        <attr name="auto_select_effect" format="boolean"></attr>
        <attr name="max_select" format="integer"></attr>
    </declare-styleable>
</resources>
